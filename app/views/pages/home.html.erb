<%= simple_form_for @contact_form do |f| %>
<div class="container-contact100">

    <%= render 'shared/map' %>
    <div class="wrap-contact100">
      <form class="contact100-form validate-form">
        <span class="contact100-form-title">
          Contactez nous
        </span>

        <div class="wrap-input100 validate-input" data-validate="Name is required">
          <span class="label-input100">Nom</span>
          <%= f.input :nom, label: false, :input_html => { :class => 'input100' }, placeholder: 'Votre nom'%>
          <span class="focus-input100"></span>
        </div>

        <div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
          <span class="label-input100">Email</span>
          <%= f.input :email, label: false, :input_html => { :class => 'input100' }, placeholder: 'Votre adresse email (ex: albator@gmail.com)' %>
          <span class="focus-input100"></span>
        </div>

        <div class="wrap-input100">
          <span class="label-input100">Téléphone</span>
          <%= f.input :prenom, label: false, :input_html => { :class => 'input100' }, placeholder: 'Votre téléphone'%>
          <span class="focus-input100"></span>
        </div>

        <div class="wrap-input100 validate-input" data-validate = "Message is required">
          <span class="label-input100">Message</span>
          <%= f.input :message, label: false, :input_html => { :class => 'input100' }, rows: 10, placeholder: 'Quel est votre projet ?' %>
          <span class="focus-input100"></span>
        </div>
        <div class="container-contact100-form-btn">
          <div class="wrap-contact100-form-btn">
            <div class="contact100-form-bgbtn"></div>
            <button class="contact100-form-btn">
              <%= f.submit "Envoyé", :class => 'form_button' %>
            </button>
          </div>
        </div>

      </form>
    </div>
  </div>
        <% end %>




















